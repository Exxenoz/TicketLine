                    <fx:include source="newsElement.fxml"/>
                    <Separator/>
                    <fx:include source="newsElement.fxml"/>
                    <Separator/>
                    <fx:include source="newsElement.fxml"/>
                    <Separator/>
                    <fx:include source="newsElement.fxml"/>
