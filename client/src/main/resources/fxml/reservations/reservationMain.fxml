<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
      styleClass="vbox"
      xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" spacing="5.0"
      fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.reservations.seating.ReservationsController">
    <children>
        <TabPane fx:id="bookingTabPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="750.0" prefWidth="800.0"
                 stylesheets="@/css/secondLevelTabs.css" tabClosingPolicy="UNAVAILABLE">
            <stylesheets>
                <URL value="@/css/bookings.css"/>
            </stylesheets>
            <tabs>
                <Tab text="%bookings.main.tab1">
                    <fx:include fx:id="bookingSearch" source="reservationTabSearch.fxml"/>
                </Tab>
            </tabs>
        </TabPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>

